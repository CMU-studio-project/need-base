{
  "sentiment-analysis": {
    "roberta": {
      "input_type": "text",
      "model": "cardiffnlp/twitter-roberta-base-sentiment",
      "label_map": {"LABEL_0": "negative", "LABEL_1": "neutral", "LABEL_2": "positive"}
    }
  },
  "automatic-speech-recognition": {
    "wav2vec2": {
      "input_type": "audio",
      "model": "facebook/wav2vec2-large-960h-lv60-self"
    }
  }
}